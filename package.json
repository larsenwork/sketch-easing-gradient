{
  "name": "sketch-easing-gradients",
  "version": "0.1.0",
  "description": "Supercharge your gradients in Sketch with no-linear color mix and custom color spaces",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/larsenwork/sketch-easing-gradient.git"
  },
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "sketch-easing-gradients",
    "manifest": "src/manifest.json",
    "main": "plugin.sketchplugin"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.1.3",
    "@skpm/extract-loader": "^1.0.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "eslint": "^4.11.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-html": "^4.0.0",
    "eslint-plugin-import": "^2.8.0",
    "file-loader": "^1.1.4",
    "html-loader": "^0.5.1",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "style-loader": "^0.19.0",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4"
  },
  "resources": [
    "resources/**/main.js"
  ],
  "dependencies": {
    "sketch-module-web-view": "^0.2.0",
    "vue": "^2.5.3"
  },
  "author": "larsenwork <andreas@larsenwork.com>",
  "license": "MIT"
}
